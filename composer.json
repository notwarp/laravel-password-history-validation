{
    "name": "notwarp/laravel-password-history-validation",
    "description": "Prevent users from reusing recently used passwords",
    "keywords": [
        "lucaterribili",
        "laravel-password-history-validation",
        "laravel",
        "password"
    ],
    "homepage": "https://github.com/lucaterribili/laravel-password-history-validation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luca Terribili",
            "email": "dottorlucaterribili@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/console": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.* || 7.* || 8.* || 9.* || 10.*",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.* || 7.* || 8.* || 9.* || 10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.* || 9.*"
    },
    "autoload": {
        "psr-4": {
            "LucaTerribili\\LaravelPasswordHistoryValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LucaTerribili\\LaravelPasswordHistoryValidation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucaTerribili\\LaravelPasswordHistoryValidation\\LaravelPasswordHistoryValidationServiceProvider"
            ]
        }
    }
}
